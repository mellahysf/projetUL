<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>GestionContact</display-name>
	<servlet>
		<servlet-name>Contact</servlet-name>
		<servlet-class>Servlets.ContactServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/Contact</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>AfficherContact</servlet-name>
		<servlet-class>Servlets.AfficherContactServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AfficherContact</servlet-name>
		<url-pattern>/AfficherContact</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Modifier</servlet-name>
		<servlet-class>Servlets.ModifierServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Modifier</servlet-name>
		<url-pattern>/modifier</url-pattern>
	</servlet-mapping>

</web-app>
